/*
재귀코드
fib(n) {
    if (n = 1 or n = 2)
    then return 1;  # 코드1
    else return (fib(n - 1) + fib(n - 2));
}

dp코드
fibonacci(n) {
    f[1] <- f[2] <- 1;
    for i <- 3 to n
        f[i] <- f[i - 1] + f[i - 2];  # 코드2
    return f[n];
}
입력
첫째 줄에 n(5 ≤ n ≤ 40)이 주어진다.

출력
코드1 코드2 실행 횟수를 한 줄에 출력한다.
*/

#include <iostream>
#include <vector>
using namespace std;

int recur = 0;
int dp = 0;

int fib(int n)
{
    if(n == 1 || n == 2){
        recur++;
        return 1;
    }
    else{
        return fib(n-1) + fib(n-2); 
    }
}

int fibonacchi(int n)
{
    vector<int> f(n+1);
    f[1] = f[2] = 1;
    for(int i = 3; i<=n;++i){
        f[i] = f[i-1]+f[i-2];
        dp++;
    }
    return f[n];
}

int main()
{
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int n;
    cin >> n;

    fib(n);
    fibonacchi(n);

    cout << recur << " " << dp;

    return 0;
}